option_settings:
 "aws:elasticbeanstalk:container:python":
   NumProcesses: 3
   NumThreads: 20
   WSGIPath: app/app/wsgi.py
   StaticFiles: /static/=static/
   MediaFiles: /static/=media/

packages:
 yum:
   postgresql95-devel: []

container_commands:
 01_migrate:
   command: "python manage.py migrate"
   leader_only: true
 02_collectstatic:
   command: "python manage.py collectstatic --noinput"
   


